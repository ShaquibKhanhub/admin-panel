{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "background-clip",
          "position: sticky",
          "scroll-behavior",
          "-webkit-tap-highlight-color",
          "text-decoration-skip-ink"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 115",
    "not ios_saf <= 16.5",
    "not safari <= 16.5",
    "not and_chr <= 114",
    "not chrome <= 115",
    "not edge <= 114",
    "not firefox <= 115",
    "not opera <= 100",
    "not samsung <= 21"
  ]
}